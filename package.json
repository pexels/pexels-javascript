  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pexels/pexels-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/pexels/pexels-javascript/issues"
  },
  "source": "src/main.ts",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "module": "dist/main.module.js",
  "unpkg": "dist/main.umd.js",
